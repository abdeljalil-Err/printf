#include "main.h"

/**
 * _printstring - function print the argument of type string.
 * @args: the arguments of string type.
 * Return: the lenth os string.
 */
int _printstring(va_list arg)
{
	int i;
	char *str = va_arg(args, char*);

	for (i = 0; str[i] != '\0'; i++)
	{
		_putchar(str[i]);
	}
	i--;
	return (i);
}
